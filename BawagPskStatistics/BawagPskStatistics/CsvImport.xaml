<Window x:Class="BawagPskStatistics.CsvImport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BawagPskStatistics"
        mc:Ignorable="d"
        Title="Import" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="AUTO"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Name="BtnSelectAndImport" Click="BtnSelectAndImport_Click" Content="Select and import" Grid.Row="0" Margin="5" Padding="5" />
    </Grid>
</Window>
